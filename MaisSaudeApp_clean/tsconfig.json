{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "App.js",
    "setup.js"
  ]
  ,
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "ios",
    "android"
  ]
}
